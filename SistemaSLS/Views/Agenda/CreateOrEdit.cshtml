    <div class="container-fluid">
        <div class="block-header">
            <h2>Agenda</h2>
        </div>
        <!-- Input -->
        <div class="row clearfix">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="card">
                    <div class="header">
                        <h2>
                            <small>Crear o editar un servicio</small>
                        </h2>
                    </div>
                    <div class="body">
                        <h2 class="card-inside-title">Basic Examples</h2>
                        <div class="row clearfix">
                            <div class="col-sm-12">
                                <div class="form-group">
                                    <div class="form-line">
                                        <input type="text" class="form-control" placeholder="Username" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="form-line">
                                        <input type="password" class="form-control" placeholder="Password" />
                                    </div>
                                </div>
                                <div class="body">
                                    <div class="row clearfix">
                                        <div class="col-sm-6"> 
                                            <select class="bootstrap-select form-control show-tick" ng-options="Pais.IdPais as Pais.Descripcion for Pais in Paises" ng-model="Pais.IdPais">
                                                <option value="">Seleccione un Pais</option>
                                                <option></option>
                                            </select>
                                        </div>
                                        
                                        <select name="Pais" [ngModel]="Pais">
                                            <option [ngValue]="null">Choose a state</option>
                                            <option *ngFor="let Pais of Paises" [ngValue]="Pais.IdPais">
                                                {{ Pais.Descripcion }}
                                            </option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

